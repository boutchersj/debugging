let x = 7;
let y = 9;
let result = "to come";

if(x == y) {
  result = "Equal!";
} else {
  result = "Not equal!";
}

console.log(result);

//Basically just look out for any mixups between the assignment operator '=' and the equality operators '==' & '===' (strict)
//They look similar, but can significantly impact any branched operation (if/else statements, for example) if swapped
