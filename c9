function raiseToPower(b, e) {
  return Math.pow(b, e);
}

let base = 2;
let exp = 3;
let power = raiseToPower(base, exp);
console.log(power);

//Always check to make sure your function arguments are declared in the right order
//If they're not and the argument value types are different, the browser will throw a runtime error
//If they ARE the same type, then the logic of the code won't make sense
