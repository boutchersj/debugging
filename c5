let myArray = [1, 2, 3];
let arraySum = myArray.reduce((previous, current) =>  previous + current);
console.log(`Sum of array values is: ${arraySum}`);

//Forgetting to include both parts of parenthetical or bracket pairs () {} [] is very common
//It often happens when editing existing code with the same type of bracket, or when nesting bracket types
//Best practice to avoid this issue is to include the bracket pair right away and begin typing between them
